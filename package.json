    "react-native": "^0.71.4",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "18.2.0",
    "rn-host-detect": "^1.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.0.2",
