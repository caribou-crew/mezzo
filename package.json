    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "^9.3.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-websocket-mock": "^2.4.0",
    "memfs": "^3.4.13",
    "node-fetch": "^3.3.1",
    "nx": "15.8.7",
