{
  "name": "mezzo",
  "version": "0.8.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caribou-crew/mezzo.git"
  },
  "scripts": {
    "start": "nx serve",
    "dev:serve": "cross-env NODE_ENV=development nx run-many --target=serve --projects=core-server,admin-web",
    "dev": "npm run build:shared-libs && npm run dev:serve",
    "build": "cross-env NODE_ENV=production nx run-many --all --exclude=workspace --target=build",
    "build:executor": "npx tsc tools/executors/buildCoreWithWeb/impl",
    "build:shared-libs": "nx run-many --target=build --projects=constants,interfaces,core-client,utils-logger,interceptor-react-native-with-reactotron",
    "build:prod": "cross-env NODE_ENV=production nx run-many --all --prod --exclude=workspace --target=build && nx prepare core-server",
    "bumpVersion:ci": "nx run workspace:version",
    "commit": "cz",
    "test": "nx run-many --all --exclude=workspace --target=test --parallel",
    "test:coverage": "nx run-many --all --exclude=workspace --target=test --parallel --coverage --coverageReporters=lcov",
    "test:ci": "npm run test:coverage && node ./tools/coverageMerger.js",
    "clean": "rm -rf node_modules/ && npm install",
    "lintAll": "nx run-many --all --exclude=workspace --target=lint --parallel",
    "testAll": "nx run-many --all --exclude=workspace --target=test --parallel",
    "format:check": "nx format:check --all --exclude=workspace ",
    "format:fix": "nx format:write --all --exclude=workspace ",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.5",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.1",
    "@nrwl/nx-cloud": "^14.0.3",
    "axios": "^0.26.1",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "express": "4.17.2",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.8.0",
    "query-string": "^7.1.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-flip-toolkit": "^7.0.13",
    "react-is": "18.1.0",
    "react-reflex": "https://github.com/caribou-crew/Re-Flex/tarball/master",
    "react-router-dom": "6.3.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.0.1",
    "reactotron-core-client": "^2.8.10",
    "regenerator-runtime": "0.13.7",
    "styled-jsx": "5.0.2",
    "tracer": "^1.1.5",
    "tslib": "^2.3.1",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@jscutlery/semver": "^2.23.4",
    "@nrwl/cli": "14.1.4",
    "@nrwl/cypress": "14.1.4",
    "@nrwl/devkit": "^14.1.7",
    "@nrwl/eslint-plugin-nx": "14.1.4",
    "@nrwl/express": "14.1.4",
    "@nrwl/jest": "14.1.4",
    "@nrwl/linter": "14.1.4",
    "@nrwl/node": "14.1.4",
    "@nrwl/react": "14.1.4",
    "@nrwl/web": "14.1.4",
    "@nrwl/workspace": "14.1.4",
    "@testing-library/react": "13.1.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/memoizee": "^0.4.7",
    "@types/node": "16.11.7",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-syntax-highlighter": "^15.5.1",
    "@types/string-similarity": "^4.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-jest": "27.5.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jest-websocket-mock": "^2.3.0",
    "memfs": "^3.4.1",
    "node-fetch": "^2.6.7",
    "nx": "14.1.4",
    "prettier": "^2.5.1",
    "react-native": "^0.68.2",
    "react-test-renderer": "18.1.0",
    "rn-host-detect": "^1.2.0",
    "supertest": "^6.2.2",
    "ts-jest": "27.1.4",
    "typescript": "~4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
