{
  "name": "mezzo",
  "version": "0.10.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caribou-crew/mezzo.git"
  },
  "scripts": {
    "start": "nx serve",
    "dev:serve": "cross-env NODE_ENV=development nx run-many --target=serve --projects=core-server,admin-web",
    "dev": "npm run build:shared-libs && npm run dev:serve",
    "build": "cross-env NODE_ENV=production nx run-many --all --exclude=workspace --target=build",
    "build:executor": "npx tsc tools/executors/buildCoreWithWeb/impl",
    "build:shared-libs": "nx run-many --target=build --projects=constants,interfaces,core-client,utils-logger,interceptor-react-native-with-reactotron,utils-generate-guid",
    "build:prod": "cross-env NODE_ENV=production nx run-many --all --prod --exclude=workspace --target=build && nx prepare core-server",
    "bumpVersion:ci": "nx run workspace:version",
    "commit": "cz",
    "test": "nx run-many --all --exclude=workspace --target=test --parallel",
    "test:coverage": "nx run-many --all --exclude=workspace --target=test --parallel --coverage --coverageReporters=lcov",
    "test:ci": "npm run test:coverage && node ./tools/coverageMerger.js",
    "coverage:merge": "node ./tools/coverageMerger.js",
    "clean": "rm -rf node_modules/ && npm install",
    "lintAll": "nx run-many --all --exclude=workspace --target=lint --parallel",
    "testAll": "nx run-many --all --exclude=workspace --target=test --parallel",
    "format:check": "nx format:check --all --exclude=workspace ",
    "format:fix": "nx format:write --all --exclude=workspace ",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@nrwl/nx-cloud": "15.2.3",
    "axios": "^1.3.4",
    "core-js": "^3.29.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.8.1",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-flip-toolkit": "^7.0.17",
    "react-is": "18.2.0",
    "react-reflex": "https://github.com/caribou-crew/Re-Flex/tarball/master",
    "react-router-dom": "6.9.0",
    "react-spinners": "^0.13.8",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.1.1",
    "reactotron-core-client": "^2.8.10",
    "regenerator-runtime": "0.13.11",
    "styled-jsx": "5.1.2",
    "tracer": "^1.1.6",
    "tslib": "^2.5.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@jscutlery/semver": "^2.30.1",
    "@nrwl/cli": "15.8.7",
    "@nrwl/cypress": "15.8.7",
    "@nrwl/devkit": "15.8.7",
    "@nrwl/eslint-plugin-nx": "15.8.7",
    "@nrwl/express": "15.8.7",
    "@nrwl/jest": "15.8.7",
    "@nrwl/js": "15.8.7",
    "@nrwl/linter": "15.8.7",
    "@nrwl/node": "15.8.7",
    "@nrwl/react": "15.8.7",
    "@nrwl/web": "15.8.7",
    "@nrwl/webpack": "15.8.7",
    "@nrwl/workspace": "15.8.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/react": "14.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/memoizee": "^0.4.8",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/string-similarity": "^4.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "babel-jest": "29.5.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.36.0",
